{
    "name": "rewear-backend",
    "version": "1.0.0",
    "description": "Community Clothing Exchange Backend API",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon src/server.ts",
        "build": "npm run css:build && tsc",
        "start": "node dist/server.js",
        "test": "jest",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "cache:clear": "redis-cli FLUSHDB",
        "seed": "ts-node scripts/seed.ts",
        "seed:build": "npm run build && node dist/scripts/seed.js",
        "css:build": "tailwindcss -i ./src/styles/input.css -o ./public/css/styles.css --minify",
        "css:watch": "tailwindcss -i ./src/styles/input.css -o ./public/css/styles.css --watch"
    },
    "keywords": [
        "clothing",
        "exchange",
        "swap",
        "community",
        "typescript",
        "express"
    ],
    "author": "ReWear Team",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "connect-flash": "^0.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.10",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "express-session": "^1.18.1",
        "express-validator": "^7.0.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "method-override": "^3.0.0",
        "mongoose": "^8.0.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "redis": "^4.6.10"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/connect-flash": "^0.0.40",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.2",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/method-override": "^3.0.0",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.8.0",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "autoprefixer": "^10.4.21",
        "eslint": "^8.50.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.17",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}